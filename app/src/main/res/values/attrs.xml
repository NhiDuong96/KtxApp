<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LinkText">
        <attr name="startIndex" format="integer"/>
        <attr name="endIndex" format="integer"/>
    </declare-styleable>

    <declare-styleable name="InputView">
        <attr name="label" format="string"/>
        <attr name="editable" format="boolean"/>
        <attr name="inputType" format="enum">
            <enum name="text" value="1"/>
            <enum name="number" value="2"/>
            <enum name="phone" value="3"/>
            <enum name="date" value="4"/>
            <enum name="password" value="128"/>
        </attr>
    </declare-styleable>
</resources>